package:
  name: vcdat
  version: @VERSION@

source:
  git_rev: @UVCDAT_BRANCH@
  git_url: git://github.com/CDAT/vcdat.git

requirements:
  build:
  - python
  - setuptools
  - nodejs
  run:
  - python=2
  - nodejs
  - flask
  - vcs
  - cdms2
  - cdutil
  - genutil
  - service_identity
  - vcsaddons
  - vcs-js

build:
  noarch_python: true

about:
  home: http://github.com/CDAT/vcdat
