{
  "name": "vCDAT",
  "version": "0.0.4",
  "description": "Front-end GUI for CDAT",
  "main": "src/js/app.js",
  "private": true,
  "dependencies": {
    "bootstrap-slider": "github:matthewma7/bootstrap-slider",
    "core-js": "^2.5.1",
    "css-element-queries": "^0.3.2",
    "jquery": "^3.1.0",
    "lodash": "^4.17.4",
    "moment": "^2.18.1",
    "pubsub-js": "^1.5.8",
    "react": "^15.1.0",
    "react-bootstrap": "^0.30.10",
    "react-bootstrap-dialog": "^0.9.1",
    "react-color": "^2.13.8",
    "react-dnd": "^2.2.4",
    "react-dnd-html5-backend": "^2.2.4",
    "react-dom": "^15.1.0",
    "react-redux": "^4.4.5",
    "react-toastify": "^3.3.1",
    "redux": "=3.5.2",
    "redux-undo": "^0.6.1",
    "vcs-widgets": "^0.0.5"
  },
  "devDependencies": {
    "babel-cli": "^6.4.0",
    "babel-core": "^6.4.0",
    "babel-loader": "^6.2.1",
    "babel-plugin-__coverage__": "^11.0.0",
    "babel-plugin-istanbul": "^4.1.5",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.3.13",
    "babel-register": "^6.26.0",
    "chai": "^3.5.0",
    "coveralls": "^3.0.0",
    "cross-env": "^5.1.1",
    "css-loader": "^0.28.4",
    "enzyme": "^2.4.1",
    "eslint": "3.2.2",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-react": "^6.1.0",
    "html-file-cov": "^1.0.0",
    "ignore-styles": "^5.0.1",
    "jsdom": "^11.4.0",
    "jsdom-global": "^3.0.2",
    "mocha": "^3.5.3",
    "mocha-jsdom": "^1.1.0",
    "node-sass": "^4.5.3",
    "nyc": "^11.3.0",
    "react-addons-test-utils": "^15.3.1",
    "redux-test-utils": "^0.2.1",
    "sass-loader": "^6.0.6",
    "sinon": "^4.1.2",
    "style-loader": "^0.18.2",
    "webpack": "^2.7.0"
  },
  "config": {
    "blanket": {
      "pattern": "src",
      "data-cover-never": [
        "dist",
        "node_modules",
        "test"
      ]
    }
  },
  "nyc": {
    "sourceMap": false,
    "instrument": false,
    "include": [
      "src/**/*.js*"
    ],
    "require": [
      "babel-register",
      "ignore-styles",
      "jsdom-global/register"
    ]
  },
  "scripts": {
    "build": "webpack --colors --progress",
    "release": "webpack -p --output-path=../backend/vcdat/resources && cp src/index.html ../backend/vcdat/resources",
    "coverage": "cross-env NODE_ENV=test nyc --extension .jsx --reporter=lcov --reporter=text npm test",
    "coveralls": "nyc report --reporter=text-lcov | coveralls",
    "test": "mocha test/mocha --grep '/.*Test.js[x]{0,1}/' --require ignore-styles --require jsdom-global/register --compilers jsx:babel-register --recursive",
    "debug_tests": "mocha test/mocha --grep '/.*Test.js[x]{0,1}/' --require ignore-styles --require jsdom-global/register --compilers jsx:babel-core/register --recursive --inspect --debug-brk",
    "lint": "$(npm bin)/eslint -f table src/*.js* src/*/*.js* src/*/*/*.js* src/*/*/*/*.js*",
    "lint_silent": "npm run lint > lint.out; vim lint.out"
  },
  "license": "",
  "repository": "http://github.com/UV-CDAT/vcdat"
}
