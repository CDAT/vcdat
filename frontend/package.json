{
  "name": "vCDAT",
  "version": "0.1.0",
  "description": "Front-end GUI for CDAT using React and Redux",
  "main": "src/js/app.js",
  "private": true,
  "dependencies": {
    "css-element-queries": "^0.3.2",
    "react": "^15.1.0",
    "react-dom": "^15.1.0",
    "react-redux": "^4.4.5",
    "redux": "^3.5.2",
    "redux-undo": "^0.6.1",
    "webpack": "^1.12.11"
  },
  "devDependencies": {
    "babel-cli": "^6.4.0",
    "babel-core": "^6.4.0",
    "babel-loader": "^6.2.1",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "chai": "^3.5.0",
    "enzyme": "^2.4.1",
    "eslint": "3.2.2",
    "eslint-plugin-react": "^6.1.0",
    "karma": "^1.1.2",
    "karma-chai": "^0.1.0",
    "karma-mocha": "^1.1.1",
    "karma-sinon": "^1.0.5",
    "mocha": "^3.0.2",
    "sinon": "^1.17.5",
    "webpack": "^1.12.11",
    "webpack-dev-server": "^1.14.1"
  },
  "scripts": {
    "build": "webpack --inline --colors --progress --display-error-details --display-cached --content-base src/",
