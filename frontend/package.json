{
  "name": "vCDAT",
  "version": "0.1.0",
  "description": "Front-end GUI for CDAT",
  "main": "src/js/app.js",
  "private": true,
  "dependencies": {
    "css-element-queries": "^0.3.2",
    "jquery": "^3.1.0",
    "lodash": "^4.17.4",
    "react": "^15.1.0",
    "react-bootstrap": "^0.30.7",
    "react-dnd": "^2.2.4",
    "react-dnd-html5-backend": "^2.2.4",
    "react-dom": "^15.1.0",
    "react-redux": "^4.4.5",
    "redux": "=3.5.2",
    "redux-undo": "^0.6.1",
    "vcs-widgets": "^0.0.4"
  },
  "devDependencies": {
    "babel-cli": "^6.4.0",
    "babel-core": "^6.4.0",
    "babel-loader": "^6.2.1",
    "babel-plugin-__coverage__": "^11.0.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.3.13",
    "babel-register": "^6.14.0",
    "chai": "^3.5.0",
    "css-loader": "^0.28.4",
    "enzyme": "^2.4.1",
    "eslint": "3.2.2",
    "eslint-plugin-react": "^6.1.0",
    "html-file-cov": "^1.0.0",
    "isparta": "^4.0.0",
    "istanbul": "^0.4.5",
    "istanbul-instrumenter-loader": "^0.2.0",
    "jsdom": "^9.4.5",
    "mocha": "^3.0.2",
    "mocha-jsdom": "^1.1.0",
    "nyc": "^8.3.0",
    "react-addons-test-utils": "^15.3.1",
    "sinon": "^1.17.5",
    "style-loader": "^0.18.2",
    "webpack": "^2.2.1"
  },
  "config": {
    "blanket": {
      "pattern": "src",
      "data-cover-never": [
        "dist",
        "node_modules",
        "test"
      ]
    }
  },
  "nyc": {
    "sourceMap": false,
    "instrument": false,
    "include": [
      "src/**/*.js*"
    ],
    "require": [
      "babel-register"
    ]
  },
  "scripts": {
    "build": "webpack --colors --progress",
    "release": "webpack -p --output-path=../backend/vcdat/resources && cp src/index.html ../backend/vcdat/resources",
    "coverage": "NODE_ENV=test nyc --reporter=lcov --reporter=text npm test",
    "test": "mocha test/mocha --grep '/.*Test.js[x]{0,1}/' --compilers jsx:babel-core/register --recursive",
    "lint": "$(npm bin)/eslint -f table src/*.js* src/*/*.js* src/*/*/*.js* src/*/*/*/*.js*",
    "lint_silent": "npm run lint > lint.out; vim lint.out"
  },
  "license": "",
  "repository": "http://github.com/UV-CDAT/vcdat"
}
